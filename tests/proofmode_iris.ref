  Σ : gFunctors
  H : invG Σ
  H0 : cinvG Σ
  H1 : na_invG Σ
  t : na_inv_pool_name
  N : namespace
  E1, E2 : coPset
  P : uPredI (iResUR Σ)
  H2 : ↑N ⊆ E2
  ============================
  _ : na_inv t N (<pers> P)%I
  "HP" : ▷ <pers> P
  --------------------------------------□
  "Hown1" : na_own t E1
  "Hown2" : na_own t (E2 ∖ ↑N)
  --------------------------------------∗
  |={⊤}=> (▷ <pers> P ∗ na_own t (E2 ∖ ↑N))
          ∗ (na_own t E2 ={⊤}=∗ na_own t E1 ∗ na_own t E2 ∗ ▷ P)
  
